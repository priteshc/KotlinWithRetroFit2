public class UtilityMapDataBean {

    String mUsername;
    String mPassword;
    String mTransactionPassword;
    String mTelecomID;
    String mSubscriberID;
    String mMobileNumber;
    String mCycleNumber;
    String mDateofBirth;
    String mGasBillNumber;
    String mBillGasGroupNo;
    String mBillingUnit;
    String mProccessingCycle;
    String mCity;
    String mAmount;
    String mTotalAmount;

    public UtilityMapDataBean(String aUsername, String aPassword, String aTransactionPassword, String aTelecomID, String aSubscriberID, String aMobileNumber, String aCycleNumber, String aDateofBirth, String aGasBillNumber, String aBillGasGroupNo, String aBillingUnit, String aProccessingCycle, String aCity, String aAmount, String aTotalAmount) {
        this.mUsername = aUsername;
        this.mPassword = aPassword;
        this.mTransactionPassword = aTransactionPassword;
        this.mTelecomID = aTelecomID;
        this.mSubscriberID = aSubscriberID;
        this.mMobileNumber = aMobileNumber;
        this.mCycleNumber = aCycleNumber;
        this.mDateofBirth = aDateofBirth;
        this.mGasBillNumber = aGasBillNumber;
        this.mBillGasGroupNo = aBillGasGroupNo;
        this.mBillingUnit = aBillingUnit;
        this.mProccessingCycle = aProccessingCycle;
        this.mCity = aCity;
        this.mAmount = aAmount;
        this.mTotalAmount = aTotalAmount;
    }

    public String getUsername() {
        return mUsername;
    }

    public String getPassword() {
        return mPassword;
    }

    public String getTransactionPassword() {
        return mTransactionPassword;
    }

    public String getTelecomID() {
        return mTelecomID;
    }

    public String getSubscriberID() {
        return mSubscriberID;
    }

    public String getMobileNumber() {
        return mMobileNumber;
    }

    public String getCycleNumber() {
        return mCycleNumber;
    }

    public String getDateofBirth() {
        return mDateofBirth;
    }

    public String getGasBillNumber() {
        return mGasBillNumber;
    }

    public String getBillGasGroupNo() {
        return mBillGasGroupNo;
    }

    public String getBillingUnit() {
        return mBillingUnit;
    }

    public String getProccessingCycle() {
        return mProccessingCycle;
    }

    public String getCity() {
        return mCity;
    }

    public String getAmount() {
        return mAmount;
    }

    public String getTotalAmount() {
        return mTotalAmount;
    }
}
