public class CommonBean {
    String mUsername;
    String mPassword;
    String mTrancode;
    String mCirclecode;
    String mObjectid;
    String mMobileno;
    String mAmount;
    String mTotalAmount;
    String mProduct;
    String mServiceType;
    String mAccountNumber;

    public CommonBean(String aUsername, String aPassword, String aTrancode, String aCirclecode, String aObjectid, String aMobileno, String aAmount, String aTotalAmount, String aProduct, String aServiceType, String aAccountNumber) {
        this.mUsername = aUsername;
        this.mPassword = aPassword;
        this.mTrancode = aTrancode;
        this.mCirclecode = aCirclecode;
        this.mObjectid = aObjectid;
        this.mMobileno = aMobileno;
        this.mAmount = aAmount;
        this.mTotalAmount = aTotalAmount;
        this.mProduct = aProduct;
        this.mAccountNumber = aAccountNumber;
        this.mServiceType = aServiceType;
    }

    public String getServiceType() {
        return mServiceType;
    }

    public String getAccountNumber() {
        return mAccountNumber;
    }

    public String getUsername() {
        return mUsername;
    }

    public String getPassword() {
        return mPassword;
    }

    public String getTrancode() {
        return mTrancode;
    }

    public String getCirclecode() {
        return mCirclecode;
    }

    public String getObjectid() {
        return mObjectid;
    }

    public String getMobileno() {
        return mMobileno;
    }

    public String getAmount() {
        return mAmount;
    }

    public String getTotalAmount() {
        return mTotalAmount;
    }

    public String getProduct() {
        return mProduct;
    }
}
