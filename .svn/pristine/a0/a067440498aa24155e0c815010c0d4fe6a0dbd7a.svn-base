package com.atishay.online.utils.network;

/**
 * Created by hrdudhat on 11/8/2016.
 */
public class ApiError<T> {
    private int mApiErrorCode;
    private T mError;

    public ApiError(int aApiErrorCode, T aError) {
        mApiErrorCode = aApiErrorCode;
        mError = aError;
    }

    public int getApiErrorCode() {
        return mApiErrorCode;
    }

    public void setApiErrorCode(int aApiErrorCode) {
        mApiErrorCode = aApiErrorCode;
    }

    public T getError() {
        return mError;
    }

    public void setError(T aError) {
        mError = aError;
    }
}
