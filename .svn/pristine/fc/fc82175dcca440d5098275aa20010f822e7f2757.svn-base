<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true"
    android:layout_alignParentTop="true"
    android:layout_gravity="center_horizontal"
    android:background="@android:color/white"
    android:isScrollContainer="true"
    android:orientation="vertical">

    <TextView
        android:id="@+id/trans_txtTiming"
        style="@style/TextStyle"
        android:text="@string/transaction_timing"/>

    <TextView
        android:id="@+id/trans_txtMsg"
        style="@style/TextStyle"
        android:text="@string/transaction_msg"
        android:textStyle="normal"/>

    <TextView
        android:id="@+id/trans_txtimps"
        style="@style/TextStyle"
        android:text="@string/transaction_imps"
        android:textStyle="normal"/>

    <TextView
        android:id="@+id/trans_txtneft"
        style="@style/TextStyle"
        android:layout_marginBottom="@dimen/spacing_standard"
        android:text="@string/transaction_neft"
        android:textStyle="normal"/>

</LinearLayout>