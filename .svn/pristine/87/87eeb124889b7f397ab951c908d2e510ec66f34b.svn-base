    <!--<com.atishay.online.views.widgets.circlemenu.CircleImageView
        android:id="@+id/main_mtranc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/menu_circle"
        android:elevation="@dimen/spacing_control_half"
        android:visibility="invisible"
        android:src="@drawable/ic_mtranc"/>-->

    <com.atishay.online.views.widgets.circlemenu.CircleImageView
        android:id="@+id/main_postpaid"
        android:layout_width="@dimen/menu_size"
        android:layout_height="@dimen/menu_size"
        android:background="@drawable/menu_circle"
        android:elevation="@dimen/spacing_control_half"
        android:src="@drawable/ic_recharge"/>

    <com.atishay.online.views.widgets.circlemenu.CircleImageView
